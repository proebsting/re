
    #! rxx x1-simple.rx x1-simple.rcx
1:  [a-zA-Z]+
2:  [\w]+
3:  [a-zA-Z ]+
4:  [-_\.a-zA-Z0-9]+
5:  [a-zA-Z0-9]+(\.[a-zA-Z0-9]+)+
    # ssn and telno
6:  [0-9][0-9][0-9]-[0-9][0-9]-[0-9][0-9][0-9][0-9]
7:  \([0-9][0-9][0-9]\)[2-9][0-9][0-9]-[0-9][0-9][0-9][0-9]
    # www and email
8:  (http://|)[a-zA-Z0-9]+(\.[a-zA-Z0-9]*)+(/[-_\.\?a-zA-Z0-9]*)*
9:  [a-zA-Z][-_\.a-zA-Z0-9]*@[a-zA-Z0-9]*(\.[a-zA-Z0-9]*)*
A:  [a-zA-Z][-_\.a-zA-Z0-9]*@[a-zA-Z0-9]*(\.[a-zA-Z0-9]*)?
B:  [a-zA-Z][-_\.a-zA-Z0-9]*@[a-zA-Z0-9]*(\.[a-zA-Z0-9]*)+

